{
  "name": "mjw-ui",
  "version": "0.0.1-alpha",
  "description": "UI for mjw (Moraj Lab workspace manager app)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npm config set '@morajlab:registry' https://npm.pkg.github.com",
    "test": "mjw test",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "mjw hooks install",
    "story": "mjw story",
    "story:build": "mjw story --build"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.52.0",
    "@morajlab/npm.react.components.bare": "0.0.1-alpha",
    "@morajlab/npm.react.styles.common": "0.0.1-alpha",
    "@morajlab/npm.react.utils.style": "^0.0.1-alpha",
    "i18next": "^21.6.6",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.15.3"
  },
  "devDependencies": {
    "@morajlab/npm.config.typescript": "0.0.1-alpha",
    "@morajlab/npm.react.types.common": "0.0.1-alpha",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.8",
    "next-transpile-modules": "^9.0.0",
    "typescript": "4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morajlab/mjw-ui.git"
  },
  "keywords": [
    "moraj",
    "moraj-lab",
    "workspace",
    "ui"
  ],
  "author": "Morteza Jamali <mortezajamali4241@gmail.com> (https://moraj.ir)",
  "bugs": {
    "url": "https://github.com/morajlab/mjw-ui/issues"
  },
  "homepage": "https://github.com/morajlab/mjw-ui#readme",
  "lint-staged": {
    "**/*": "mjw format --type prettier",
    "**/*.css": "mjw lint --type stylelint"
  }
